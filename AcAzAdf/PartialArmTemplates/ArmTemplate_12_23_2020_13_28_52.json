{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AcAzAdf"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureSqlDatabaseSAEC')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "AzureSqlDatabase",
				"description": "Temp db connection",
				"parameters": {
					"vaultBaseUrl": {
						"type": "string",
						"defaultValue": "https://azurekeyvaultname.vault.azure.net/"
					}
				},
				"annotations": [],
				"typeProperties": {
					"connectionString": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=saazs-v-pwrbidev.database.windows.net;Initial Catalog=saec-powerbi-dev;User ID=saec_AcctechUser",
					"password": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "AzureKeyVault",
							"type": "LinkedServiceReference",
							"parameters": {
								"baseUrl": {
									"value": "@linkedService().vaultBaseUrl",
									"type": "Expression"
								}
							}
						},
						"secretName": "saec"
					}
				}
			},
			"dependsOn": []
		}
	]
}