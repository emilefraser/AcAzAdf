{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AcAzAdf"
		},
		"AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage'"
		},
		"AzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase'"
		},
		"ls_MachineLearning_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'ls_MachineLearning'"
		},
		"AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "@{linkedService().baseUrl}"
		},
		"ls_MachineLearning_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "87cfcf1d-a755-46d8-9fad-1038ad31fa27"
		},
		"ls_MachineLearning_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "MLService"
		},
		"ls_MachineLearning_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "2e126bef-c545-4f28-8b62-ddfaf854b0ad"
		},
		"ls_MachineLearning_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "34aa038a-43ff-4f1c-8315-4d477a9cfe29"
		},
		"SqlServer_OnPremises_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "acaznevm"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AcAzAdf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage_connectionString": {
						"value": "[parameters('AzureBlobStorage_connectionString')]"
					},
					"ls_MachineLearning_servicePrincipalKey": {
						"value": "[parameters('ls_MachineLearning_servicePrincipalKey')]"
					},
					"AzureSqlDatabase_connectionString": {
						"value": "[parameters('AzureSqlDatabase_connectionString')]"
					},
					"AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"ls_MachineLearning_properties_typeProperties_subscriptionId": {
						"value": "[parameters('ls_MachineLearning_properties_typeProperties_subscriptionId')]"
					},
					"ls_MachineLearning_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('ls_MachineLearning_properties_typeProperties_resourceGroupName')]"
					},
					"ls_MachineLearning_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('ls_MachineLearning_properties_typeProperties_servicePrincipalId')]"
					},
					"ls_MachineLearning_properties_typeProperties_tenant": {
						"value": "[parameters('ls_MachineLearning_properties_typeProperties_tenant')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "AcAzAdf_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"SqlServer_OnPremises_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('SqlServer_OnPremises_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'AcAzAdf_ArmTemplate_0')]"
			]
		}
	]
}