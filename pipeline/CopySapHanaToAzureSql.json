{
	"name": "CopySapHanaToAzureSql",
	"properties": {
		"description": "Pipe to move SAP HANA data to Azure Sql",
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "SapHanaSource",
						"partitionOption": "None"
					},
					"sink": {
						"type": "AzureSqlSink",
						"tableOption": "autoCreate"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SapHanaDataset",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "dataset_AzureSqlServer",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "8_Extract"
		},
		"annotations": []
	}
}